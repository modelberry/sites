{
  "name": "@jaccomeijer/modelberry-com",
  "version": "6.25.4",
  "private": true,
  "description": "https://www.modelberry.com",
  "keywords": [
    "any",
    "cli",
    "component",
    "contentful",
    "gatsbyjs",
    "netlify",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/jaccomeijer/modelberry#readme",
  "bugs": {
    "url": "https://github.com/jaccomeijer/modelberry/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaccomeijer/modelberry.git"
  },
  "license": "MIT",
  "author": "Jacco Meijer (https://www.jaccomeijer.nl)",
  "contributors": [
    {
      "name": "Jacco Meijer",
      "email": "jacco@jaccomeijer.nl",
      "url": "https://www.jaccomeijer.nl"
    }
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all clean build:* copy-static-public",
    "build:compile": "NODE_ENV=production gatsby build",
    "build:lint": "eslint 'src/**/*.{ts,tsx}'",
    "build:test": "jest --silent --passWithNoTests",
    "clean": "gatsby clean && rimraf build/*",
    "copy-static-public": "cp -r public build",
    "dev": "NODE_ENV=development gatsby develop",
    "dev-ssl": "NODE_ENV=development gatsby develop --https",
    "serve": "gatsby serve",
    "static-css-vars": "rollup --config rollup.static.config.js"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "15.0.0",
    "@emotion/css": "11.1.3",
    "@modelberry/any": "6.25.4",
    "@modelberry/css-theme": "6.25.3",
    "facepaint": "1.2.1",
    "gatsby": "3.4.1",
    "gatsby-source-graphql": "3.5.0",
    "react-bootstrap-icons": "1.5.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.11.1"
  },
  "mobelberry": {
    "version": "7.0.0"
  }
}