{
  "name": "@wheelroom/css-theme",
  "version": "6.24.3",
  "description": "Library for working with css theme variables",
  "files": [
    "CHANGELOG.md",
    "build"
  ],
  "exports": {
    "./commands/release-command.js": {
      "require": "./build/commands/release-command.cjs.js",
      "import": "./build/commands/release-command.esm.js"
    },
    "./commands/link-command.js": {
      "require": "./build/commands/link-command.cjs.js",
      "import": "./build/commands/link-command.esm.js"
    },
    "./commands/list-command.js": {
      "require": "./build/commands/list-command.cjs.js",
      "import": "./build/commands/list-command.esm.js"
    }
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:compile": "rollup --config",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:test": "jest --silent --passWithNoTests",
    "build:types": "tsc -p tsconfig.packages.json --declaration --emitDeclarationOnly --declarationDir build",
    "clean": "rimraf build/*",
    "clean-npm": "find . -name node_modules | xargs rm -rf && find . -name package-lock.json | xargs rm -rf",
    "dev": "rollup --config --watch"
  },
  "peerDependencies": {
    "react": "17.x",
    "react-dom": "17.x"
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*"
      ]
    }
  }
}
